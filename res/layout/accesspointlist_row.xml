<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="wrap_content" android:padding="5dp">
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/id"
		android:visibility="invisible"></TextView>
	<ImageView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/signalStrengthImage"></ImageView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/bestDbm"
		android:textSize="12sp" android:text="dBm" android:layout_below="@+id/signalStrengthImage"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_below="@+id/bestDbm"
		android:id="@+id/channel" android:text="channel"></TextView>
	<TextView android:text="@+id/TextView01" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/ssid"
		android:textSize="22sp" android:layout_centerInParent="true"
		android:layout_alignParentTop="true"></TextView>
	<ImageView android:id="@+id/ImageView01"
		android:layout_below="@id/ssid" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignParentRight="true"
		android:layout_alignParentTop="true"></ImageView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/encryption"
		android:text="WEP" android:layout_below="@+id/ImageView01"
		android:layout_alignParentRight="true"></TextView>
</RelativeLayout>